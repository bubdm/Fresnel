@{
    Layout = null;
    var version = Environment.TickCount.ToString();
}

<!DOCTYPE html>
<html ng-app="fresnelApp">
<head>
    <meta name="viewport" content="width=device-width" />
    
    <script src="~/scripts/angular.js"></script>
    <script src="~/scripts/_app/FresnelApp.js?v=@version"></script>
    <title>Index</title>
</head>
<body>
    <div ng-controller="toolboxController">
        Angular JS Demo <br />
        Input :  <input type="text" ng-model="text" /><br />
        {{text}}

        <div>
            {{message.title}}

            <button ng-click="loadClassHierarchy()">Hit me</button>
        </div>

        <div ng-repeat="item in classHierarchy">
            {{ item.Name }} {{item.IsEnabled }}

        </div>
    </div>
</body>
</html>